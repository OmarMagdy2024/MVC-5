}

<h1 class="mt-4">Update Employee</h1>
<br />
<br />
<form asp-action="Update">
    <partial name="_EmployeeParial"></partial>
    <input type="submit" value="Update Employee" class="btn btn-outline-success pt-2 pb-2 pl-5 pr-5 mt-3" />
